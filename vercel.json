{
  "routes": [
    {
      "src": "/api/monthly/reset",
      "dest": "/api/monthly/reset"
    },
    {
      "src": "/(.*)/monthly",
      "dest": "/api/monthly"
    },
    {
      "src": "/api/daily/reset",
      "dest": "/api/daily/reset"
    },
    {
      "src": "/(.*)/daily",
      "dest": "/api/daily"
    },
    {
      "src": "/(.*)",
      "dest": "/api/total"
    }
  ],
  "env": {
    "MONGODB_URI": "@mongodb",
    "COLLECTION": "projects"
  }
}
